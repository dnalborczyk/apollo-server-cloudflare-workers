{
  "name": "apollo-server-cloudflare-workers",
  "type": "module",
  "scripts": {
    "build": "rimraf build && NODE_ENV=production node worker.build.js",
    "typecheck": "tsc --noEmit",
    "deploy": "wrangler publish",
    "generate": "graphql-codegen --config ./codegen.cjs",
    "lint": "eslint .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@apollo/server": "^4.2.2",
    "apollo-server-integrations-cloudflare-workers": "^0.1.2",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@graphql-codegen/cli": "^2.15.0",
    "@graphql-codegen/typescript": "^2.8.2",
    "@graphql-codegen/typescript-resolvers": "^2.7.7",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "esbuild": "^0.15.16",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "patch-package": "^6.5.0",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3",
    "wrangler": "^2.5.0"
  }
}
